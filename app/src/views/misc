
    each page in config.pages
      if page.type == 'page'
        link(rel='stylesheet', href=page.layout.css)
      else if page.type == 'page_sequence'
        each page_sequence_page in page.pages
          link(rel='stylesheet', href=page_sequence_page.layout.css)

  body
    each page in config.pages
      div.page-container
        if page.type == 'page'
          | !{ include(page.layout.pug, page.data) }
        else if page.type == 'page_sequence'
          div Page Sequence
